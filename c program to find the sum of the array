#include <stdio.h>
{
  int arr[10],i,n,sum=0;
  printf("enter the size of an array :");
  scanf("%d",&n);
  printf("enter the elements of an array :");
  for(i=0;i<n;i++)
  {
     scanf("%d",&arr[i]);
  }
  for(i=0;i<n;i++)
  {
    sum=sum+arr[i];
    printf("the sum of the given array is :",sum);
  }
  return 0;
}
