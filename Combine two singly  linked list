#include <stdio.h>
#include <stdlib.h>
struct Node
{
  int data;
  struct Node* next;
};
struct Node* combineLists(struct Node* head1, struct Node* head2)
{
  if (head1 == NULL) 
  {
    return head2;
  }
  if (head2 == NULL) 
  {
    return head1;
  }
  struct Node* temp = head1;
  while (temp->next != NULL) 
  {
    temp = temp->next;
  }
  temp->next = head2;
  return head1;
}
